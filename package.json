{
  "name": "advisor-board-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest --run src/test/e2e",
    "test:integration": "vitest --run src/test/integration",
    "test:performance": "vitest --run src/test/performance",
    "test:visual": "vitest --run src/test/visual",
    "test:accessibility": "vitest --run **/*.accessibility.test.*",
    "test:all": "npm run test:e2e && npm run test:integration && npm run test:performance && npm run test:visual && npm run test:accessibility",
    "test:summary": "node scripts/test-summary.js",
    "docs:serve": "npx serve docs -l 3001"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "jspdf": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.4",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^1.2.0"
  }
}
